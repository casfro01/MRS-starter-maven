      <TextField fx:id="txtMovieSearch" layoutX="10.0" layoutY="14.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
      <Button layoutX="170.0" layoutY="10.0" mnemonicParsing="false" onAction="#createNewMovie" prefHeight="26.0" prefWidth="85.0" text="Create New" />
      <Button fx:id="updatebtn" layoutX="256.0" layoutY="10.0" mnemonicParsing="false" onAction="#updateMovie" prefHeight="26.0" prefWidth="85.0" text="Update" visible="false" disable="true" />
   </children>
